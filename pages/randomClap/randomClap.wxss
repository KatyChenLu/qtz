/* pages/randomClap/randomClap.wxss *//* my/editaddress/editaddress.wxss */

page{
  background: #f8f8f8;
  font-size: 24rpx;
}
.phone_box{
  display: flex;
}
.phone_btn{
  color: #000;
  background-color: green;
  width: 150rpx;
  height: 46rpx;
  padding: 0;
  
}
.phone_btn::after {
  border: none;
}
.phone_right{
  display: flex;
  position: relative;
}
.phone_btn_text{
  position: absolute;
  top:0;
  right:0;
color: #fff;
height: 100%;
width: 100%;
text-align: center;
font-size: 24rpx;
line-height: 46rpx;
z-index: 99;
}
.forms {
  background: #fff;
  box-sizing: border-box;
  padding:0 30rpx;
  padding-bottom: 0rpx;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.weui-cell__bd {
  border-bottom: 1px solid #f8f8f8;
  /* padding: 10rpx 0; */
  /* display:flex; */
  align-items:center;
  margin: 10rpx 0 40rpx;

}
.li_title{
  font-weight: 700;
  font-size: 28rpx;
  margin-bottom: 20rpx;
}

.evaluate_word {
  font-size: 28rpx;
  padding: 20rpx 0rpx;
  width: 100%;
  background-color: #f7eaea;
}

.picker {
  position: relative;
  color: #4d4d4d;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 30rpx;
}
.tip_view{
  width: 100%;
  color: #999;
  font-size: 24rpx;
  margin-top: 20rpx;
 }
/* .picker::after {
  content: '>';
  position: absolute;
  right: 0;
  top: 50%;
  transform: translateY(-50%);
} */

.quan {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
}

.quan_left {
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 30rpx 0rpx;
}

.quan_left>image {
  width: 36rpx;
  height: 36rpx;
}

.bind_quan {
  width: 36rpx;
  height: 36rpx;
  border: 1px solid rgba(204, 204, 204, 1);
  border-radius: 50%;
}

.quan_left view:nth-child(2) {
  padding-left: 30rpx;
}



.default {
  padding:0 30rpx;
  background: #fff;
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #4d4d4d;
  height:80rpx;
}

.preservation {
  width: 93%;
  border: 1px solid #fc4548;
  margin: 50rpx auto;
  text-align: center;
  border-radius: 10rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fc4548;
  font-size: 20px;
  font-weight: bold;
}
.weui-cell__bd .weui-input{
  width: 100%;
}
.weui-input{
  font-size: 26rpx;
  height: 30rpx;
}

 .optClass{
     background-color: #fff;
     box-sizing: border-box;
     font-size: 32rpx;
     color: #333;
     display: flex;
     align-items: center;
     padding:0 30rpx;
     flex-wrap:wrap;
 }
 .optClass .optName{
     width: 200rpx;
 }
 .optClass .optName text,.optClass .mesTit text{
     color: #ce0a0a;
 }
 .optClass .optContent{
     flex: 1;
     text-align: right;
     color: #666;
     font-size: 32rpx;
     font-size:24rpx;
     color:#999;
 }
 
 .optClass .uploadimgbox{
     flex: 0 0 33.333%;
     position: relative;
     box-sizing: border-box;
     padding: 10rpx;
     margin-bottom: 10rpx;
 }
 .optClass .uploadimgbox .progress{
     width: 100%;
     height: 100%;
     position: absolute;
     top: 0;
     left: 0;
     background-color: rgba(0, 0, 0, .5);
     color: #fff;
     font-size: 30rpx;
     text-align: center;
     line-height: 208rpx;
     z-index: 99;
 }
 .optClass .uploadimgbox .delbtn{
     width: 50rpx;
     height: 50rpx;
     position: absolute;
     top: 10rpx;
     right: 10rpx;
     background-color: rgba(0, 0, 0, .5);
     color: #fff;
     font-size: 30rpx;
     text-align: center;
     line-height: 50rpx;
     z-index: 98;
 }
 .optClass .uploadimg,.optClass .uploadbtn{
     display: block;
     width: 100%;
     height: 208rpx;
 }
 
 .subBtnBox .subBtn{
     width: 80%;
     display: block;
     margin: 80rpx auto;
     background-color: #ce0a0a;
     color: #fff;
     font-size: 32rpx;
 }
 .subBtnBox .subBtn.button-hover{
     background-color: #bb0909;
 }
 /* 上传的图片 */
