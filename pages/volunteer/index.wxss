.content .bc {
  width: 100%;
}

.content .content-box .login-btn {
  width: 688rpx;
  height: 150rpx;
  position: relative;
  margin: 50rpx auto;
}

.content .content-box .login-btn .btn-in {
  position: absolute;
  height: 98rpx;
  width: 680rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 36rpx;
  background-color: white;
  color: #1E948A;
  bottom: 0;
  display: flex;
  justify-content: center;
  flex-direction: column;
  border-radius: 10rpx;
}

.content .content-box .login-btn image {
  position: relative;
  z-index: 2;
  width: 100%;
  height: 100%;
}

.content .content-box .one {
  margin-top: 100rpx;
}

.content .code {
  width: 650rpx;
  padding: 60rpx 30rpx 30rpx 30rpx;
  box-sizing: border-box;
  font-size: 36rpx;
  text-align: center;
}

.content .code image {
  width: 500rpx;
  height: 500rpx;
}
