.login-btn {
  width: 688rpx;
  height: 150rpx;
  position: fixed;
  bottom: 31rpx;
  left: 50%;
  transform: translateX(-50%);
}

.login-btn .btn-in {
  position: absolute;
  height: 98rpx;
  width: 680rpx;
  margin: 0 auto;
  text-align: center;
  font-size: 32rpx;
  color: white;
  background-color: #1E948A;
  bottom: 0;
  display: flex;
  justify-content: center;
  flex-direction: column;
  border-radius: 10rpx;
}

.login-btn image {
  position: relative;
  z-index: 2;
  width: 100%;
  height: 100%;
}
