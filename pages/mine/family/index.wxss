.content {
  padding: 31rpx;
  box-sizing: border-box;
  height: 100vh;
  background-color: #F1F4F9;
}

.content .title {
  font-size: 32rpx;
}

.content .title text {
  position: relative;
  padding-left: 24rpx;
}

.content .title text::after {
  display: inline-block;
  content: "";
  width: 9rpx;
  border-radius: 30rpx;
  height: 100%;
  background-color: #1E948A;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 0;
}

.content .code {
  background-color: white;
  padding: 24rpx;
  box-sizing: border-box;
  margin: 32rpx 0;
  justify-content: center;
  display: flex;
  font-size: 32rpx;
}

.content .code .img {
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-right: 14rpx;
}

.content .code .img image {
  width: 30rpx;
  height: 30rpx;
  vertical-align: bottom;
}

.content .code .text {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.content .go {
  font-size: 32rpx;
  color: #409EFF;
}
